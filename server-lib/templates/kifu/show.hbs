{{#*inline "page"}}
  <div id="kifu_id" hidden>{{kifu}}</div>
  <div id="websocket_url" hidden>{{websocket_url}}</div>
  <div class="row">
    <div class="col-md-6">
      <canvas id="board" width="600" height="640" style="width:100%">
        Your brower is not supported.
      </canvas>
    </div>

    <div class="col-md-6">
      <!--
      <button class="btn" id="down_button"><i class="fa fa-angle-down" aria-hidden="true"></i></button>
      <button class="btn" id="up_button"><i class="fa fa-angle-up" aria-hidden="true"></i></button>
      <button class="btn" id="prev_button"><i class="fa fa-angle-left" aria-hidden="true"></i></button>
      <button class="btn" id="next_button"><i class="fa fa-angle-right" aria-hidden="true"></i></button>
      -->
      <table style="font-size:80%;" class="table-bordered">
        <tbody>
        <tr>
          <td id="move-2"></td>
          <td id="score-2"></td>
          <td id="diff-2"></td>
          <td id="pv-2"></td>
        </tr>
        <tr>
          <td id="move-1"></td>
          <td id="score-1"></td>
          <td id="diff-1"></td>
          <td id="pv-1"></td>
        </tr>
        <tr>
          <td id="move0"></td>
          <td id="score0"></td>
          <td id="diff0"></td>
          <td id="pv0"></td>
        </tr>
        <tr>
          <td id="move1"></td>
          <td id="score1"></td>
          <td id="diff1"></td>
          <td id="pv1"></td>
        </tr>
        <tr>
          <td id="move2"></td>
          <td id="score2"></td>
          <td id="diff2"></td>
          <td id="pv2"></td>
        </tr>
        </tbody>
        <tfoot>
          <th>指手</th>
          <th>評価値</th>
          <th>手の評価</th>
          <th>読み筋</th>
        </tfoot>
      </table>
      <p><span id="n">0</span>手目の<span id="k">0</span>手先</p>
    </div>
  </div>
{{/inline}}

{{~> layout title="kifu" ~}}
